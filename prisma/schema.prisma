// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  // provider = "postgresql"
  // url      = env("DATABASE_URL")
  provider  = "sqlite"
  url = "file:./dev.db"
}

model Note {
  id String @id @default(cuid())
  note String
  active Boolean
  createTime DateTime
  updateTime DateTime
  bgColor String @default("#ffcf7d")
}
